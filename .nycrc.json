{
  "extends": "@istanbuljs/nyc-config-typescript",
  "temp-directory": "./coverage/.nyc_output",
  "all": true,
  "extension": [".ts", ".tsx", ".html"],
  "reporter": ["lcov", "text"],
  "cache": true,
  "check-coverage": true,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "statements": 100
}
